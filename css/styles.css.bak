@charset "UTF-8";

/* Cascading Style Sheet for http://www.betterwayyet.com/ and certain other domains.
   To be used for All Media (display/monitor/print).
   Copyright World Peace Society and Growth Network. All rights reserved.
   Last revised: December 17, 2010 */

a:link {
    background-color: inherit;
	color: #000000;
	font-family: Arial, Helvetica, Verdana, Helvetica, "Trebuchet MS", sans-serif;
	font-size: 9pt;
	text-decoration: none;
}

a:visited {
	background-color: inherit;
	text-decoration: none;
	color: #990133;
}

a:active {
	background-color: inherit;
 	color: #000000;
}

a:hover {
	background-color: inherit;
 	color: #bb0500;
}

table {
	background-color: #FFFFFF;
	color: #000000;
 	font-family: Verdana, Helvetica, "Trebuchet MS", sans-serif;
	font-size: 9pt;
}

td {
	background-color: inherit;
	color: #000000;
 	font-family: Arial, Helvetica, "MS Sans Serif", Verdana, sans-serif;
	font-size: 9pt;
}

body {
	background-color: #FFFFFF;
	color: #000000;
	font-family: Verdana, Helvetica, "Trebuchet MS", sans-serif;
	font-size: 9pt;
}

.footer {
	background-color: inherit;
	color: #999999;
	font-size: 11px;
}
